{
  "manifest_version": 2,
  "name": "Myo App",
  "description": "Yay!",
  "version": "0.0.1",
  "permissions": ["geolocation", "webview"],
  "webview": {
    "partitions": [
      {
        "name": "trusted*",
        "accessible_resources": ["*"]
      }
    ]
  },
  "app": {
    "background": {
      "scripts": ["build/background.js"]
    }
  },
  "sandbox": {
    "pages": ["build/main.html"]
  }
}
