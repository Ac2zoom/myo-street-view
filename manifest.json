{
  "manifest_version": 2,
  "name": "Myo Street View",
  "description": "Use the Myo to explore Google Street View!",
  "version": "0.0.2",
  "permissions": ["geolocation", "webview"],
  "webview": {
    "partitions": [
      {
        "name": "trusted*",
        "accessible_resources": ["*"]
      }
    ]
  },
  "app": {
    "background": {
      "scripts": ["src/background.js"]
    }
  },
  "sandbox": {
    "pages": ["src/main.html"]
  }
}
